<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MagicOnion.Server" />
		<PackageReference Include="Serilog.AspNetCore" />
		<PackageReference Include="Serilog.Sinks.MSSqlServer" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\AdventureWorks.Authentication\AdventureWorks.Authentication.MagicOnion.Server\AdventureWorks.Authentication.MagicOnion.Server.csproj" />
		<ProjectReference Include="..\..\AdventureWorks.Authentication\AdventureWorks.Authentication\AdventureWorks.Authentication.csproj" />
		<ProjectReference Include="..\..\AdventureWorks.Purchasing\AdventureWorks.Authentication.AspNetCore\AdventureWorks.Authentication.AspNetCore.csproj" />
		<ProjectReference Include="..\..\AdventureWorks.Purchasing\AdventureWorks.Hosting.Server\AdventureWorks.Hosting.Server.csproj" />
		<ProjectReference Include="..\..\AdventureWorks.Purchasing\AdventureWorks.MagicOnion.Server\AdventureWorks.MagicOnion.Server.csproj" />
		<ProjectReference Include="..\AdventureWorks.Database\AdventureWorks.Database.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Properties\Resources.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <Generator>ResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <None Update="Serilog.json">
	    <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
	    <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
	  </None>
	</ItemGroup>

</Project>