<UserControl x:Class="AdventureWorks.Purchasing.View.Menu.MenuPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:fa="http://schemas.fontawesome.com/icons/fonts/properties"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             xmlns:local="clr-namespace:AdventureWorks.Purchasing.View.Menu"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance {x:Type local:MenuDesignViewModel}, IsDesignTimeCreatable=True}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <local:MenuItemButton Icon="OpencartBrands"
                                MenuLabel="再発注"
                                Command="{Binding NavigateRePurchasingCommand}"/>
    </Grid>
</UserControl>
